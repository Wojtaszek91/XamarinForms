<?xml version="1.0" encoding="utf-8" ?>
<TabbedPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:views="clr-namespace:Laboratoria"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:d="http://xamarin.com/schemas/2014/forms/design"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d"
             x:Class="Laboratoria.TabMainPage"
             >
  <!--Pages can be added as references or inline-->
    <NavigationPage Title="Home" IconImageSource="baseline_home_black_24">
        <x:Arguments>
            <views:HomePage/>
        </x:Arguments>
    </NavigationPage>
    <NavigationPage Title="Settings" IconImageSource="baseline_home_black_24">
        <x:Arguments>
            <views:Settings/>
        </x:Arguments>
    </NavigationPage>
    <ContentPage Title="Tab 1" />
  <ContentPage Title="Tab 2" />
  <ContentPage Title="Tab 3" />
</TabbedPage>